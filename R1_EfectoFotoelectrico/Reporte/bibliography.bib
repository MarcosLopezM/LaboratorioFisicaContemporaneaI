@article{martinez-perdigueroCommonMistakeDescription2019,
  title = {On a Common Mistake in the Description of the Photoelectric Effect},
  volume = {57},
  issn = {0031-921X},
  url = {https://doi.org/10.1119/1.5131119},
  doi = {10.1119/1.5131119},
  abstract = {The photoelectric effect is one of the key experiments taught during first- or second-year university and high school modern physics courses. It is usually the first experiment to introduce light quantization and the concept of photons as “packets of energy.” Here, we want to point out a widespread mistake concerning the interpretation of the saturation current at constant light intensity that is found even in some classic hardback literature. Although this is usually overlooked, it can weaken the conclusions a student can draw from the correct understanding of the experiment.},
  pages = {536--537},
  number = {8},
  journaltitle = {The Physics Teacher},
  shortjournal = {The Physics Teacher},
  author = {Martinez-Perdiguero, Josu},
  urldate = {2025-02-22},
  date = {2019-11-01},
}

