@article{boysPhotoelectricEffectRevisited1978,
  title = {Photoelectric effect revisited (or an inexpensive device to determine h/e)},
  volume = {46},
  issn = {0002-9505},
  url = {https://doi.org/10.1119/1.11371},
  doi = {10.1119/1.11371},
  abstract = {A novel and inexpensive device for introductory students to measure Planck's constant using the photoelectric effect has been developed. Quasimonochromatic light illuminates the photocathode. An inexpensive voltage follower circuit with high impedance and low capacitance is used to determine the stopping potential, which is read on a standard voltmeter. The results agree to within ±5\% of the currently accepted value. The experimental results are highly reproducible.},
  pages = {133--135},
  number = {2},
  journaltitle = {American Journal of Physics},
  shortjournal = {American Journal of Physics},
  author = {Boys, Donald W. and Cox, Mary E. and Mykolajenko, William},
  urldate = {2025-02-22},
  date = {1978-02-01},
}

@article{caulfieldCesiumAntimonyFilms1966,
  title = {Cesium—Antimony Films in Equilibrium with Cesium Vapor},
  volume = {37},
  issn = {0021-8979},
  url = {https://doi.org/10.1063/1.1708167},
  doi = {10.1063/1.1708167},
  abstract = {Photoemissive yield, thermionic emission, contact potential difference with clean tungsten, and electrical resistance measurements were made on cesium—antimony films in equilibrium with cesium vapor. A search was made for those combinations of cesium—antimony temperatures {TCs}‐Sb and cesium reservoir temperatures {TCs} which mark discontinuous changes in the properties of these films. The loci of those {TCs}‐Sb‐{TCs} points mark the boundaries of distinct Cs‐Sb ``states.'' Three clearly separable states, the familiar photo‐sensitive state and its two neighboring states, were studied in detail. The first state occurred for {TCs}‐Sb only slightly greater than the cesium reservoir temperature {TCs}. The state is characterized by a low maximum photoemissive yield (0.04 electrons/photon), moderate work function (1.65 {eV}), and high resistivity proportional to exp[−0.4 ({eV})/{kTCs}‐Sb]. The material in this state is p‐type Cs3Sb with adsorbed cesium metal. The second state arises after a rapid transition from the first state as {TCS}‐Sb is increased further. This state is characterized by a high maximum photoemissive yield (up to 0.23 electrons/photon), moderate work function (1.60 {eV}), and resistivity proportional to exp[−1.0 ({eV})/{kTCS}‐Sb]. This material is Cs3Sb without appreciable adsorbed cesium. From prior measurements of cesium partial pressure we obtain a cesium—antimony temperature T0 at which the film is Cs3Sb at its cesium deficiency limit. The third state occurs abruptly as {TCs}‐Sb exceeds T0. The state for {TCs}‐Sb \&gt; T0 is a less cesium‐rich phase (probably Cs2Sb). This state is characterized by a low maximum photoemissive yield (0.04 electrons/photon), low work function (1.35 {eV}), and a higher resistance than the previous state.},
  pages = {4927--4935},
  number = {13},
  journaltitle = {Journal of Applied Physics},
  shortjournal = {Journal of Applied Physics},
  author = {Caulfield, H. J. and Chapman, R. A.},
  urldate = {2025-02-22},
  date = {1966-12-01},
}

